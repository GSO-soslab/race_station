<?xml version="1.0"?>

<launch>

  <arg name="robot_name" default="race_station"/>
  <!-- Bring up the microstrain ahrs  -->
    <include file="$(find race_station_bringup)/launch/include/microstrain.launch.xml">
        <arg name="robot_name" value="$(arg robot_name)"/>
    </include> 

   <!-- Bring up madgwick filter-->
    <include file="$(find race_station_bringup)/launch/include/madgwick_filter.launch.xml">
        <arg name="robot_name" value="$(arg robot_name)"/>
    </include> 
</launch>